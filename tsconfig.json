{
  "compilerOptions": {
    "target": "es2015",
    "outDir": "./dist/",
    "pretty": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "strict": true,
    "module": "es6",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "commonTypes": ["src/commonTypes"],
      "API": ["src/API"]
    }
  },
  "include": [
    "./src/"
  ],
  "exclude": [
    "./main.js"
  ]
}